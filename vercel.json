{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/location",
      "dest": "server/index.js?path=/location"
    },
    {
      "src": "/gpt",
      "dest": "server/index.js?path=/gpt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
