{
  "version": 2,
  "builds": [
    {
      "src": "src/server/index.js",
      "use": "@vercel/node",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/index.js"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://chatgpt-translation.vercel.app/"
  }
}
